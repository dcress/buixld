/* Start of Header area */
     <div class="header-area" id="content">
      <div class="container">
        <div class="row">
            <div class="col-md-12">
                <i class="icon-tasks pull-left"></i><h2>{{ pageName }}</h2>
              </div>
        </div>
      </div>
    </div>
    /* End of Header Area */

     <!-- Start of Main Content Area -->
    <div class="container health-records-container" id="recentVisitListing">
      <div class="row">
        <div class="col-md-8 health-records-content" role="main">
          <div class="recent-visits-paragraph info-label-value">
            <p>Listed below is information about your most recent visits. Click links to access additional details about a specific visit. Click <strong>Summary of Care</strong> to access a list of all your visits to this facility and information about each.</p>
          </div>
        </div><!-- End of col-md-8 -->

        <div class="col-md-4">
          <div class="recent-visits-pinboard less-margin">
            <div class="row access-area">
              <div class="col-md-12">
                <i class="icon-plus-sign"></i>
              </div>
              <div class="col-md-12">
                <div class="bold-text">ACCESS YOUR ACCOUNT</div>
            </div>
            <div class="col-md-12">
                <div class="regular-text">SUMMARY of CARE</div>
            </div>
            <div class="col-md-6">
              <a class="btn btn-primary"><i class="icon-download-alt"></i>Download</a>
            </div>
            <div class="col-md-6">
              <a class="btn btn-primary"><i class="icon-share"></i>Transmit</a>
            </div>
        </div>
        </div>
      </div> <!-- End of col-md-4 -->
    </div> <!-- End of Main Content Area row -->
  </div> <!-- End of Health Records Content -->

    <hr />

     <!-- Listing Info -->
     {{#visitInfo}}
    <div class="recent-visit-listing container" id="{{id}}">
      <div class="row">
        <div class="col-md-12">
          <div class="visit-info">
            <a href="recent-visit-detailed.html"><div class="visit-info-date pull-left">
              <h3>{{visitDate}}</h3>
            </div>
            <div class="hyphen pull-left"><h3>&nbsp;-&nbsp;</h3></div>
            <div class="visit-info-type pull-left">
              <h3>{{visitType}}</h3>
            </div></a>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-md-12 listing-info">
          <div class="row">
            <div class="col-md-6">
              <div class="row">
                <div class="col-md-6 listing-facility-info">
                  <div class="info-label-head">FACILITY</div>
                  <div class="info-label-value">{{facilityName}}</div>
                </div>
                <div class="col-md-6 listing-contact-info">
                  <div class="info-label-head">CONTACT</div>
                  <div class="info-label-value">{{contactNo}}</div>
                </div>
                <div class="col-md-5 listing-address-info">
                  <div class="info-label-head">YOUR ADDRESS</div>
                  <div class="info-label-value">{{address}}</div>
                </div>
                <div class="col-md-6"></div>
              </div>
            </div>
            <div class="col-md-6 listing-reason-info">
              <div class="info-label-head">REASON</div>
              <div class="info-label-value"><p>{{reason}}</p></div>
            </div>
          </div>
        </div>
      </div>
    </div><!-- End of listing Info -->
    {{/visitInfo}}

