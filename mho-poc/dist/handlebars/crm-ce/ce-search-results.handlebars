
{{#if result}}

<!-- Results listing -->
<section id="ce-results-listing">

  <!-- Results criteria -->
  <article id="ce-results-criteria" class="container-sm">
    <div class="row">
	<!-- Resutls header section -->
	<div class="col-md-12">
		<h2>Class &amp; Events Search Results</h2>
	</div>
</div>
		
<div class="row btn-row">
	<!-- Resutls sort button -->

 	<div class="col-xs-12 col-sm-4 left">
		<select name="sort" id="select-sort" class="form-control select-styled" required="required">
			<option value="startDateTime">Sort by: Start Date</option>
			<option value="eventName">Sort by: Event Name</option>
			<option value="eventClassType">Sort by: Event Type</option>
			{{{createSelectOption '"distance"' 'Sort by: Distance' }}} 
		</select>
	</div>
	
	<div class="col-xs-12 col-sm-2 right">
		<div class="btn-group">
			<select name="results" id="select-results" class="form-control select-styled" required="required">
				<option value="10">10</option>
				<option value="20">20</option>
			</select>
		</div>
	</div>
</div>
  </article>

  <!-- Results -->
  <section id="ce-result-container" class="container-sm">
    
{{#result}}

<article class="ce-result">

  <!-- Event title location date and time -->
  <div id="ce-result-info" class="row">

    <!-- Event Title -->
    <div class="col-sm-8 ce-result-title">
      <h3><a data-toggle="modal" data-target="#ce-modal-result-details{{ eventId}}">{{ eventName}}</a></h3>
      <p class="eventDescription">{{truncate eventDescription 100}}</p>
      <p><i class="fa fa-pencil"></i> <a>{{ eventClassType}}</a></p>

      <!-- Location information -->
      <div class="row ce-result-location">
        <div class="col-xs-3 ce-result-location-img" >
          <i class="fa fa-map-marker fa-4x" style="text-align: center; display: block;"></i>
        </div>
        <div class="col-sm-9 ce-result-location-info">
          {{{truncateDistance distance}}}
          {{{sessionLocation locationLat locationLong locationName locationStreetAddress locationCity locationState locationZipCode}}}
        </div>
      </div>

    </div>

    <!-- Calendar and Time -->
    <div class="col-sm-4 ce-result-date-time">
      <div class="row">
        <div class="col-sm-12 ce-result-calendar"><div id="ce-date-calendar">
	{{{calendarIcon startDateTime}}}
</div></div>
        <div class="col-sm-12 ce-result-time">
          {{{eventDate startDateTime endDateTime}}} 
        </div>
      </div>
    </div>

  </div>

<!-- Result Attributes -->
  <div id="ce-result-attributes" class="row">
    <!-- Result details -->
    <div class="col-sm-3 ce-result-attribute ce-result-details">
      <a data-toggle="modal" data-target="#ce-modal-result-details{{ eventId}}"><i class="fa fa-calendar"></i> See Full Details</a>
    </div>

    <!-- Availability -->
    <div class="col-sm-3 ce-result-attribute ce-result-available">
      <span><i class="fa fa-check-circle"></i> {{available}}</span>
    </div>

    <!-- Cost -->
    <div class="col-sm-2 ce-result-attribute ce-result-cost">
      <span class="ce-alert-default"><i class="fa fa-money"></i> 
        ${{registrationCost}}
      </span>
    </div>

    <!-- Register for event -->
    <div class="col-sm-4 ce-result-cta">
      <a href="../../register/" class="btn btn-success"><i class="fa fa-check"></i> Register for Event</a>
    </div>

    <!-- Call to register
    <div class="col-sm-4 ce-result-cta">
      <a href="../../register/" class="btn btn-info"><i class="fa fa-phone"></i> Call: (123) 456-7890</a>
    </div> -->

    <!-- Joing waitlist
    <div class="col-sm-4 ce-result-cta">
      <a href="../../register/" class="btn btn-default"><i class="fa fa-flag"></i> Join Waitlist</a>
    </div> -->

    <!-- Toggle payment modal
    <div class="col-sm-4 ce-result-cta">
      <a data-toggle="modal" data-target="#ce-modal-payment" class="btn btn-success"><i class="fa fa-check"></i> Toggle Payment Modal</a>
    </div> -->

  </div>

</article>


{{/result}}


