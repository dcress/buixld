<div class="modal fade ce-modal-result-details" id="ce-modal-result-details{{eventId}}" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">

    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="ce-modal-dismiss close" data-dismiss="modal" aria-hidden="true"><i class="fa fa-times-circle"></i></button>
        <h3 class="modal-title" id="myModalLabel">Event Details</h3>
      </div>

      <div class="modal-body">


        <!-- Result -->
        <article class="ce-result">

          <!-- Event title location date and time -->
          <div id="ce-result-info" class="row">

            <!-- Event Title -->
            <div class="col-sm-8 ce-result-title">
              <h3><a data-toggle="modal" data-target="#ce-modal-result-details{{!-- {{sessions.0.eventId}} --}}">{{eventName}}</a></h3>
              <p class="eventDescription">{{truncate eventDescription 100}}</p>
              <p><i class="fa fa-pencil"></i> <a>{{eventClassType}}</a></p>

              <!-- Location information -->
            <div class="row ce-result-location">
              <div class="col-xs-3 ce-result-location-img">
                <i class="fa fa-map-marker fa-4x" style="text-align: center; display: block;"></i>
              </div>
              <div class="col-sm-9 ce-result-location-info">         
                {{{truncateDistance distance}}}
                {{{sessionLocation locationLat locationLong locationName locationStreetAddress locationCity locationState locationZipCode}}}
              </div>
              </div>

            </div>

           <!-- Calendar and Time -->
            <div class="col-sm-4 ce-result-date-time">
              <div class="row">
                <div class="col-sm-12 ce-result-calendar"><div id="ce-date-calendar">
	{{{calendarIcon startDateTime}}}
</div></div>
                <div class="col-sm-12 ce-result-time">
                  {{{eventDate startDateTime endDateTime}}} 
  
                </div>
              </div>
            </div>

          </div>

          <!-- Result Attributes -->
          <div id="ce-result-attributes" class="row">
            <!-- Result details -->
            <div class="col-sm-3 ce-result-attribute ce-result-details">
              <a data-toggle="modal" data-target="#ce-modal-result-details"><i class="fa fa-calendar"></i> See Full Details</a>
            </div>

            <!-- Availability -->
            <div class="col-sm-3 ce-result-attribute ce-result-available">
              <span class="ce-alert-success"><i class="fa fa-check-circle"></i> {{available}}</span>
            </div>

            <!-- Cost -->
            <div class="col-sm-2 ce-result-attribute ce-result-cost">
              <span class="ce-alert-default"><i class="fa fa-money"></i> 
                ${{registrationCost}}</span>
            </div>

            <!-- Register for event -->
            <div class="col-sm-4 ce-result-cta">
              <a href="../../register/" class="btn btn-success"><i class="fa fa-check"></i> Register for Event</a>
            </div>

            <!-- Call to register
            <div class="col-sm-4 ce-result-cta">
              <a href="../../register/" class="btn btn-info"><i class="fa fa-phone"></i> Call: (123) 456-7890</a>
            </div> -->

            <!-- Joing waitlist
            <div class="col-sm-4 ce-result-cta">
              <a href="../../register/" class="btn btn-default"><i class="fa fa-flag"></i> Join Waitlist</a>
            </div> -->

            <!-- Toggle payment modal
            <div class="col-sm-4 ce-result-cta">
              <a data-toggle="modal" data-target="#ce-modal-payment" class="btn btn-success"><i class="fa fa-check"></i> Toggle Payment Modal</a>
            </div> -->

          </div>
        </article>

        <!-- Tabs for content -->
        <ul class="nav nav-tabs">
          <li class="active"><a href="#description" data-toggle="tab">Description</a></li>
          <li><a href="#occurances" data-toggle="tab">Class Occurances</a></li>
          <li><a href="#notes" data-toggle="tab">Notes</a></li>
        </ul>
        <!-- Tab content -->
        <div class="tab-content">
          <div class="tab-pane fade in active" id="description">
            <p class="ce-extra-info"><span class="ce-extra-header">INSTRUCTOR:</span> {{eventMainContactName}}</p>
            <p class="ce-extra-info"><span class="ce-extra-header">TYPE:</span>  {{eventClassType}}</p>
            <p> {{eventDescription}} </p>
          </div>
          <div class="tab-pane fade" id="occurances">
            <p class="ce-extra-info"><span class="ce-extra-header">INSTRUCTOR:</span>{{eventMainContactName}}</p>
            <p class="ce-extra-info"><span class="ce-extra-header">TYPE:</span>  {{eventClassType}}</p>
          <p>  {{eventDescription}}</p>
          </div>
          <div class="tab-pane fade" id="notes">
            <p> {{eventEnrollmentCriteria}}</p>
          </div>
        </div>
      </div>

      <div class="modal-info">
        <div class="modal-info-text">
          <span><i class="fa fa-info-circle"></i> Need Help?</span>
          <span class="modal-info-text-phone">{{eventEnrollmentPhoneNumber}}</span>
        </div>
      </div>
    </div>
  </div>
</div>

{{/result}}


